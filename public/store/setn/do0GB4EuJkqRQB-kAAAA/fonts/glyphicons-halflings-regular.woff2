<script>
    $(document).ready(function () {

        var headerH = $(".header-area").height() + $(".top-branding-area").height();
        $(".mainmenu-area").data("offset-top", headerH);

        var _active = $(".mainMenu.active");
        if (_active.find(".top-second-nav").length > 0) {
            $(".menuH").css("margin-top", "36px");
        }
        //SetSubMenu(_active, true);

        $(".mainMenu").hover(
          function () {
              $(".mainMenu").css("background-color", "transparent").find(".top-second-nav").hide();

              $(this).removeAttr("style").find(".top-second-nav").show();

              if ($(this).find(".top-second-nav").length > 0 || $(".mainMenu.active").find(".top-second-nav").length > 0) {
                  $(".menuH").css("margin-top", "36px");
              }
              else {
                  $(".menuH").removeAttr("style");
              }
              //SetSubMenu($(this));
          }, function () {
              $(".mainMenu").removeAttr("style").find(".top-second-nav").removeAttr("style");
              if ($(this).find(".top-second-nav").length > 0 && _active.find(".top-second-nav").length <= 0) {
                  $(".menuH").removeAttr("style");
              }
              //SetSubMenu($(".mainMenu.active"), true);
          }
        );

        //maintop-area
    });
</script>

